{
    "name": "web-fetch",
    "version": "1.3.2",
    "description": "Asset Crawler for common Web pages",
    "keywords": [
        "asset",
        "crawler",
        "browser",
        "web"
    ],
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "homepage": "https://tech-query.me/Web-fetch/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechQuery/Web-fetch.git"
    },
    "bugs": {
        "url": "https://github.com/TechQuery/Web-fetch/issues"
    },
    "source": "source/index.ts",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "bin": {
        "web-fetch": "dist/command.js"
    },
    "scripts": {
        "prepare": "husky install",
        "test": "lint-staged",
        "debug": "rm -rf test/  &&  mkdir test  &&  cd test/  &&  ts-node ../source/command.tsx",
        "build": "rm -rf dist/  &&  tsc  &&  typedoc source/",
        "prepublishOnly": "npm test  &&  npm run build",
        "install": "app-find chrome -c",
        "start": "typedoc source/  &&  open-cli docs/index.html"
    },
    "lint-staged": {
        "*.{md,json,yml,ts}": "prettier --write",
        "*.ts": "eslint --fix"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@tech_query/node-toolkit": "^1.2.1",
        "@types/jsdom": "^21.1.1",
        "array-unique-proposal": "^0.3.4",
        "commander-jsx": "^0.6.5",
        "file-type": "^16.5.4",
        "fs-extra": "^11.1.1",
        "fs-match": "^1.7.1",
        "jsdom": "^21.1.2",
        "lodash.memoize": "^4.1.2",
        "node-fetch": "^2.6.11",
        "puppeteer-core": "^20.7.3",
        "tslib": "^2.5.3",
        "turndown": "^7.1.2",
        "turndown-plugin-gfm": "^1.0.2",
        "yaml": "^2.3.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.1",
        "@types/lodash.memoize": "^4.1.7",
        "@types/node": "^18.16.18",
        "@types/node-fetch": "^2.6.4",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "open-cli": "^7.2.0",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typedoc": "^0.24.8",
        "typedoc-plugin-mdn-links": "^3.0.3",
        "typescript": "~5.0.4"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "singleQuote": true
    }
}
