{
    "compilerOptions": {
        "outDir": "dist/",
        "declaration": true,
        "target": "ES2017",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "jsx": "react-jsx",
        "jsxImportSource": "commander-jsx",
        "importHelpers": true,
        "skipLibCheck": true,
        "lib": ["ES2020", "DOM", "DOM.Iterable"]
    },
    "include": ["source/*", "*.ts"],
    "typedocOptions": {
        "name": "Web fetch",
        "excludeExternals": true,
        "excludePrivate": true,
        "readme": "./ReadMe.md",
        "plugin": ["typedoc-plugin-mdn-links"]
    }
}
