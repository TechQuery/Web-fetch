{
    "name": "web-fetch",
    "version": "1.4.1",
    "description": "Asset Crawler for common Web pages",
    "keywords": [
        "asset",
        "crawler",
        "browser",
        "web"
    ],
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "homepage": "https://tech-query.me/Web-fetch/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechQuery/Web-fetch.git"
    },
    "bugs": {
        "url": "https://github.com/TechQuery/Web-fetch/issues"
    },
    "source": "source/index.ts",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "bin": "dist/command.js",
    "scripts": {
        "prepare": "husky",
        "test": "lint-staged",
        "debug": "rm -rf test/  &&  mkdir test  &&  cd test/  &&  tsx ../source/command.tsx",
        "build": "rm -rf dist/  &&  tsc  &&  typedoc source/",
        "prepublishOnly": "npm test  &&  npm run build",
        "install": "app-find chrome msedge firefox -c",
        "start": "typedoc source/  &&  open-cli docs/index.html"
    },
    "lint-staged": {
        "*.{md,json,yml,ts}": "prettier --write",
        "*.ts": "eslint --fix"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@tech_query/node-toolkit": "^2.0.0-alpha.0",
        "@types/jsdom": "^21.1.7",
        "array-unique-proposal": "^0.3.4",
        "commander-jsx": "^0.6.9",
        "dotenv": "^16.4.5",
        "file-type": "^16.5.4",
        "fs-extra": "^11.2.0",
        "fs-match": "^1.7.2",
        "jsdom": "^25.0.1",
        "lodash.memoize": "^4.1.2",
        "puppeteer-core": "^23.7.1",
        "tslib": "^2.8.1",
        "turndown": "^7.2.0",
        "turndown-plugin-gfm": "^1.0.2",
        "yaml": "^2.6.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/fs-extra": "^11.0.4",
        "@types/lodash.memoize": "^4.1.9",
        "@types/node": "^20.17.6",
        "@types/turndown": "^5.0.5",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^15.12.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "open-cli": "^8.0.0",
        "prettier": "^3.3.3",
        "tsx": "^4.19.2",
        "typedoc": "^0.26.11",
        "typedoc-plugin-mdn-links": "^3.3.7",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.14.0"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "singleQuote": true
    }
}
