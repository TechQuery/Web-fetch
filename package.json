{
    "name": "hexo-migrator-web",
    "version": "1.0.0",
    "description": "Hexo Migrator plugin for common Web pages",
    "keywords": [
        "hexo",
        "migrate",
        "plugin",
        "web"
    ],
    "license": "GPL-3.0",
    "author": "shiy2008@gmail.com",
    "homepage": "https://github.com/TechQuery/hexo-migrator-web#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechQuery/hexo-migrator-web.git"
    },
    "bugs": {
        "url": "https://github.com/TechQuery/hexo-migrator-web/issues"
    },
    "main": "dist/index.js",
    "bin": {
        "web-fetch": "dist/command.js"
    },
    "scripts": {
        "test": "lint-staged",
        "build": "rm -rf dist/  &&  tsc",
        "prepublishOnly": "npm test  &&  npm run build",
        "install": "app-find chrome -c"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm run build"
        }
    },
    "lint-staged": {
        "*.{md,json,yml}": [
            "prettier --write"
        ],
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@tech_query/node-toolkit": "^1.0.1",
        "file-type": "^14.3.0",
        "fs-extra": "^9.0.0",
        "fs-match": "^1.4.0",
        "jsdom": "^16.2.2",
        "node-fetch": "^2.6.0",
        "puppeteer-core": "3.0.4",
        "turndown": "^6.0.0",
        "turndown-plugin-gfm": "^1.0.2",
        "yaml": "^1.9.2"
    },
    "peerDependencies": {
        "hexo": "^4.2.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.1.0",
        "@types/hexo": "^3.8.4",
        "@types/jsdom": "^16.2.1",
        "@types/node-fetch": "^2.5.7",
        "@types/puppeteer-core": "^2.0.0",
        "@types/turndown": "^5.0.0",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "prettier": "^2.0.5",
        "typescript": "^3.9.2"
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "singleQuote": true
    }
}
