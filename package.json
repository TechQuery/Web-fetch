{
    "name": "hexo-migrator-web",
    "version": "0.4.6",
    "description": "Hexo Migrator plugin for common Web pages",
    "keywords": [
        "hexo",
        "migrate",
        "plugin",
        "web"
    ],
    "license": "GPL-3.0",
    "author": "shiy2008@gmail.com",
    "homepage": "https://github.com/TechQuery/hexo-migrator-web#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechQuery/hexo-migrator-web.git"
    },
    "bugs": {
        "url": "https://github.com/TechQuery/hexo-migrator-web/issues"
    },
    "main": "dist/index.js",
    "engines": {
        "node": "^6.13.0"
    },
    "scripts": {
        "lint": "lint-staged",
        "pack": "babel source/ -d dist/ -s --ignore source/index.js  &&  cat source/index.js > dist/index.js",
        "build": "npm run lint  &&  npm run pack",
        "prepublishOnly": "npm run build",
        "install": "app-find chrome -c"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run build"
        }
    },
    "lint-staged": {
        "*.{html,md,css,less,json,yml,yaml}": [
            "prettier --write",
            "git add"
        ],
        "*.js": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "@babel/runtime": "^7.5.5",
        "@tech_query/node-toolkit": "^0.9.5",
        "file-type": "^11.1.0",
        "fs-extra": "^8.1.0",
        "fs-match": "^1.3.5",
        "jsdom": "^15.1.1",
        "node-fetch": "^2.6.0",
        "puppeteer-autoscroll-down": "^0.1.1",
        "puppeteer-core": "1.7.0",
        "turndown": "^5.0.3",
        "turndown-plugin-gfm": "^1.0.2",
        "yaml": "^1.6.0"
    },
    "peerDependencies": {
        "hexo": "^3.9.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "eslint": "^6.1.0",
        "husky": "^3.0.2",
        "lint-staged": "^9.2.1",
        "prettier": "^1.18.2"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "6.13.0"
                    }
                }
            ]
        ],
        "plugins": [
            "@babel/plugin-transform-runtime"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    }
}
