{
    "name": "web-fetch",
    "version": "1.5.0",
    "description": "Asset Crawler for common Web pages",
    "keywords": [
        "asset",
        "crawler",
        "browser",
        "web"
    ],
    "license": "LGPL-3.0",
    "author": "shiy2008@gmail.com",
    "homepage": "https://tech-query.me/Web-fetch/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechQuery/Web-fetch.git"
    },
    "bugs": {
        "url": "https://github.com/TechQuery/Web-fetch/issues"
    },
    "source": "source/index.ts",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "bin": "dist/command.js",
    "scripts": {
        "prepare": "husky",
        "test": "lint-staged",
        "debug": "rm -rf test/  &&  mkdir test  &&  cd test/  &&  tsx ../source/command.tsx",
        "pack-dist": "rm -rf dist/  &&  tsc  &&  mv dist/source/* dist/  &&  rm -rf dist/eslint.* dist/source",
        "pack-docs": "rm -rf docs/  &&  typedoc source/",
        "build": "npm run pack-dist  &&  npm run pack-docs",
        "prepublishOnly": "npm test  &&  npm run build",
        "install": "app-find chrome msedge firefox -m 1 -c",
        "start": "npm run pack-docs  &&  open-cli docs/index.html"
    },
    "lint-staged": {
        "*.{md,json,yml,ts}": "prettier --write",
        "*.ts": "eslint --fix"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "@tech_query/node-toolkit": "2.0.0-beta.0",
        "@types/jsdom": "^27.0.0",
        "array-unique-proposal": "^0.3.4",
        "commander-jsx": "^0.7.1",
        "dotenv": "^17.2.3",
        "file-type": "^16.5.4",
        "fs-extra": "^11.3.2",
        "fs-match": "^2.0.0",
        "jsdom": "^27.1.0",
        "lodash.memoize": "^4.1.2",
        "puppeteer-core": "^24.27.0",
        "tslib": "^2.8.1",
        "turndown": "^7.2.2",
        "turndown-plugin-gfm": "^1.0.2",
        "web-utility": "^4.6.4",
        "yaml": "^2.8.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/fs-extra": "^11.0.4",
        "@types/lodash.memoize": "^4.1.9",
        "@types/node": "^22.18.13",
        "@types/turndown": "^5.0.6",
        "eslint": "^9.39.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "jiti": "^2.6.1",
        "lint-staged": "^16.2.6",
        "open-cli": "^8.0.0",
        "prettier": "^3.6.2",
        "tsx": "^4.20.6",
        "typedoc": "^0.28.14",
        "typedoc-plugin-mdn-links": "^5.0.10",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.2"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "singleQuote": true
    }
}
